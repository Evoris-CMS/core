{
  "name": "evoris/core",
  "version": "0.1.0",
  "description": "Core package for Evoris",
  "authors": [
    {
      "name": "Thomas Hanke",
      "email": "thomas@han.ke"
    }
  ],
  "require": {
    "php": ">=8.3",
    "patchlevel/event-sourcing": "^3.12",
    "symfony/serializer": "^6.4",
    "symfony/http-foundation": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/config": "^6.4",
    "patchlevel/event-sourcing-bundle": "^3.12",
    "twig/twig": "^3.21",
    "symfony/routing": "^6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.3"
  },
  "autoload": {
    "psr-4": {
      "Evoris\\Core\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Evoris\\Core\\Tests\\": "tests/"
    }
  }
}
